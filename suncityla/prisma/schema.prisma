datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

// This is an example schema. You can use it as a starting point to create your own schema.
model Admin {
    id       Int     @id @default(autoincrement())
    username String  @unique
    password String
    name     String?
}
